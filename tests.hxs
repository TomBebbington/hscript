function factorial(n) {
	var x = 1;
	for(i in 2...n)
		x *= i;
	return x;
}
trace(factorial(10));

var Vector = {
	new: function(x, y) {
		return {x: x, y: y};
	},
	magnitude: function(v) {
		return Math.sqrt(v.x * v.x + v.y * v.y);
	}
};

var vector = Vector.new(0, 1);
trace(Vector.magnitude(vector));
trace(vector.x);