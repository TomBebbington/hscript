function measure(f, times:Int) {
	var o = haxe.Timer.stamp();
	for(i in 0...times)
		f();
	trace("Took "+(haxe.Timer.stamp() - o)+" seconds to run "+times+" tests");
}

class Vector {
	public var x:Float;
	public var y:Float;
	public function new(x, y):Void {
		this.x = x;
		this.y = y;
	}
}
trace(new Vector(0, 0));